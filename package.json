{
	"name": "n8n-nodes-telegram-user",
	"version": "0.1.0",
	"description": "",
	"keywords": [
		"n8n-community-node-package",
		"telegram"
	],
	"license": "MIT",
	"homepage": "https://github.com/allenday",
	"author": {
		"name": "Allen Day",
		"email": "allenday@github.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/allenday/n8n-nodes-telegram-user.git"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
		"test": "npm run test:unit && npm run test:integration",
		"test:unit": "jest --config jest.config.js",
		"test:integration": "jest --config jest.integration.config.js"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/TelegramUserCredentialsApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/TelegramUserNode/TelegramUserNode.node.js"
		]
	},
	"devDependencies": {
		"@types/jest": "^29.5.12",
		"@typescript-eslint/eslint-plugin": "^8.25.0",
		"@typescript-eslint/parser": "~8.25.0",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-n8n-nodes-base": "^1.11.0",
		"eslint-plugin-prettier": "^5.2.3",
		"gulp": "^4.0.2",
		"jest": "^29.7.0",
		"jsonc-eslint-parser": "^2.4.0",
		"prettier": "^3.5.2",
		"ts-jest": "^29.1.2",
		"typescript": "~4.8.4"
	},
	"dependencies": {
		"@types/node": "^22.13.5",
		"gulp-cli": "^3.0.0",
		"n8n-workflow": "^1.70.0"
	}
}
